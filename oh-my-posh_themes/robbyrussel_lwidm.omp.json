{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "always",
            "format": "(\ue235 {0} )"
          },
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "➜",
          "foreground": "#98C379",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " {{ .Path }} ",
          "foreground": "#56B6C2",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>)</>",
          "foreground": "#D0666F",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ✗",
          "foreground": "#BF616A",
          "type": "status",
          "style": "plain"
        },
        {
          "properties": {
            "ahead_color": "#ffcc00",
            "ahead_icon": "\u2191",
            "behind_color": "#00ccff",
            "behind_icon": "\u2193",
            "branch_icon": "\ue725",
            "branch_symbol": "",
            "cache_duration": "none",
            "staged_icon": "✔",
            "staging_color": "#00ff00",
            "unstaged_icon": "✘",
            "untracked_color": "#999999",
            "untracked_icon": "\u25cf",
            "working_color": "#ff0000"
          },
          "type": "poshgit",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
